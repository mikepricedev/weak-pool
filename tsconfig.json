{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "base",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "target": "ES2022",
    "lib": ["ES2022"],
    "declaration": false,
    "noEmit": true,
    "module": "ES2022",
    "moduleResolution": "Node"
  },
  "include": ["./src/**/*.ts"]
}
